<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Fragment.YearFragment">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="이번년도 버킷리스트"
        android:gravity="center">

    </TextView>

    <LinearLayout
        android:layout_width="320dp"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:id="@+id/yearlayout"
        android:orientation="vertical"
        android:background="@drawable/yearbg">

        <include layout="@layout/year_item" android:id="@+id/item1" />
        <include layout="@layout/year_item" android:id="@+id/item2" />
        <include layout="@layout/year_item" android:id="@+id/item3" />
        <include layout="@layout/year_item" android:id="@+id/item4" />
        <include layout="@layout/year_item" android:id="@+id/item5" />
        <include layout="@layout/year_item" android:id="@+id/item6" />
        <include layout="@layout/year_item" android:id="@+id/item7" />
        <include layout="@layout/year_item" android:id="@+id/item8" />
        <include layout="@layout/year_item" android:id="@+id/item9" />
        <include layout="@layout/year_item" android:id="@+id/item10" />

    </LinearLayout>

</RelativeLayout>
